        "    # 1. 時間刻み\n",
        "    dt = T / N\n",
        "\n",
        "    # 2. 時間軸\n",
        "    t = np.linspace(0, T, N + 1)\n",
        "\n",
        "    # 3. 標準正規乱数\n",
        "    Z = np.random.normal(0, 1, N)\n",
        "\n",
        "    # 4. Brownian motion\n",
        "    W = np.zeros(N + 1)\n",
        "    W[1:] = np.cumsum(np.sqrt(dt) * Z)\n",
        "\n",
